CREATE TABLE person_175 (
	personId integer CONSTRAINT id_key_person PRIMARY KEY,
	lastName varchar(20),
	firstName varchar(20)
);

CREATE TABLE address_175 (
	addressId integer CONSTRAINT id_key_address PRIMARY KEY,
	personId integer,
	city varchar(20),
	state varchar(20)
);

